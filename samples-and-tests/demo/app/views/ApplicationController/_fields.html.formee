*{#{formee_field atm?.pin_encrypt_des, fieldName:'atm.pin_encrypt_des', maxlength:'1', fieldRequired:false, inputClass:'box_shadow' /}}*

<div class="field">
#{field 'atm.pin_encrypt_des'}
    <label>&{field.name }</label>
%{ value = field.value ?: flash[field.name] }%
    <input type="text" id="${field.id}" name="${field.name}" value="${value}" />
    <span class="error">#{error field.name /}</span>
#{/field}
</div>

